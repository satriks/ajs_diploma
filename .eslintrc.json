{
    "extends": "standard",
    "env": {
       "es6": true,
       "browser": true,
       "jest": true
    },
    "parserOptions": {
      "ecmaVersion": 13
    },
    "rules": {
      "no-useless-call": "off",
      "max-len": ["error",{  "code": 140,  "ignorePattern": "^import"}],
      "no-restricted-syntax": [
        "error",
        "LabeledStatement",
        "WithStatement"
      ]
    }
  }